<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <display-name>rcamis</display-name>
    <welcome-file-list>
        <welcome-file>login.php</welcome-file>
    </welcome-file-list>
    <servlet>
        <description>login as user</description>
        <display-name>Login</display-name>
        <servlet-name>Login</servlet-name>
        <servlet-class>rw.ac.rca.webapp.web.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login.php</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>create course</description>
        <display-name>CreateCourse</display-name>
        <servlet-name>CreateCourse</servlet-name>
        <servlet-class>rw.ac.rca.webapp.web.CreateCourse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateCourse</servlet-name>
        <url-pattern>/createcourse.php</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>ListCourse</display-name>
        <servlet-name>ListCourse</servlet-name>
        <servlet-class>rw.ac.rca.webapp.web.ListCourse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListCourse</servlet-name>
        <url-pattern>/listcourse.php</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>create user</description>
        <display-name>CreateUser</display-name>
        <servlet-name>CreateUser</servlet-name>
        <servlet-class>rw.ac.rca.webapp.web.CreateUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateUser</servlet-name>
        <url-pattern>/createuser.php</url-pattern>
    </servlet-mapping>
    <servlet>
        <description>register user</description>
        <display-name>Register</display-name>
        <servlet-name>Register</servlet-name>
        <servlet-class>rw.ac.rca.webapp.web.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register.php</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>ListUser</display-name>
        <servlet-name>ListUser</servlet-name>
        <servlet-class>rw.ac.rca.webapp.web.ListUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListUser</servlet-name>
        <url-pattern>/listuser.php</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>DashBoard</display-name>
        <servlet-name>DashBoard</servlet-name>
        <servlet-class>rw.ac.rca.webapp.web.Dashboard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DashBoard</servlet-name>
        <url-pattern>/dashboard.php</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Search</servlet-name>
        <display-name>Search</display-name>
        <description></description>
        <servlet-class>rw.ac.rca.webapp.web.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/search.php</url-pattern>
    </servlet-mapping>
</web-app>